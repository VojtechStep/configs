" Edit scroll speed
bind j scrollline 2
bind k scrollline -2

" Tab navigation
bind J tabnext_gt
bind K tabprev

" Open macros
bind o fillcmdline open
bind O fillcmdline tabopen

bind go current_url open
bind gO current_url tabopen

" Press space twice to switch to last tab
bind <Space><Space> tab #
bind <Space>; fillcmdline tab

bind x composite tabnext; tabclose; tab #

set csp clobber
set hintuppercase false
colors dark

